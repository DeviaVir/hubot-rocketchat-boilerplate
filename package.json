{
  "name": "hubot-rocketchat-boilerplate",
  "version": "0.0.0-develop",
  "description": "An example Hubot demonstrating usage of the Rocket.Chat adaptor.",
  "main": "bin/hubot",
  "repository": "git@github.com:RocketChat/hubot-rocketchat-boilerplate.git",
  "author": "Tim Kinnane <tim.kinnane@rocket.chat>",
  "license": "MIT",
  "private": false,
  "keywords": [
    "hubot",
    "rocketchat",
    "chatbot",
    "chat"
  ],
  "engines": {
    "node": "> 8.0.0",
    "npm": "> 5.0.0"
  },
  "dependencies": {
    "coffeescript": "^2.2.2",
    "dotenv": "^5.0.0",
    "hubot": "https://github.com/hubotio/hubot.git#v3.0.1",
    "hubot-rocketchat": "timkinnane/hubot-rocketchat#convert-es6"
  },
  "scripts": {
    "shell": "coffee --nodejs '-r dotenv/config' node_modules/.bin/hubot -a shell",
    "local": "coffee --nodejs '-r dotenv/config' node_modules/.bin/hubot -a rocketchat",
    "debug": "coffee --nodejs '--inspect --inspect-brk -r dotenv/config' node_modules/.bin/hubot -a rocketchat"
  }
}
